/* Test legal downcasts */

class A {
	int a;
	int b;
	void print(){
		a = 5;
		b = 5;
		write(a);
		write(b);
		writeln();
	}
		
}
class B extends A {
	//int a;
	int b;
	void print(){
		a = 6;
		b = 6;
		write(a);
		write(b);
		writeln();
	}
	
}

class Main {
   void main() {
      A a;
      B b;
      a = new B();
      b = (B) a;   /* OK at runtime */
      write(0);
      writeln();
      a.print();
      b.print();
      
   }
}
